{
  "name": "server",
  "version": "0.0.1",
  "description": "projects",
  "license": "MIT",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "18 || 20 || 22"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t server .",
    "docker:run": "docker run -p 3000:3000 -d server",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "start2": "node dist/index.js",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "dev": "tsc-watch -b --onSuccess \"npm run start\"",
    "prod": "node ."
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Nttdev <tainguyen.ntt.97@gmail.com>",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^11.0.6",
    "@loopback/authentication-jwt": "^0.15.6",
    "@loopback/boot": "^7.0.6",
    "@loopback/core": "^6.1.3",
    "@loopback/repository": "^7.0.6",
    "@loopback/rest": "^14.0.6",
    "@loopback/rest-explorer": "^7.0.6",
    "@loopback/service-proxy": "^7.0.6",
    "@vercel/node": "^3.2.24",
    "dotenv": "^16.4.7",
    "loopback-connector-mysql": "^7.0.16",
    "loopback-connector-postgresql": "^7.1.7",
    "multer": "^1.4.5-lts.1",
    "next": "14.2.10",
    "react": "^18",
    "slugify": "^1.6.6",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.6",
    "@loopback/eslint-config": "^15.0.4",
    "@loopback/testlab": "^7.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^16.18.101",
    "eslint": "^8.57.0",
    "sass": "^1.89.2",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.1",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.2.0",
    "typescript": "~5.2.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
